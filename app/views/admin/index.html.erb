<h1>Admin</h1>

<%= will_paginate @toilets %>

<table id="results">
  <tr>
    <th>name</th>
    <th>address</th>
    <th>venue type</th>
    <th>toilet location</th>
    <th>who can use</th>
    <th>how to access</th>
    <th>changing bench</th>
    <th>hoist</th>
    <th>description</th>
    <th>longitude</th>
    <th>latitude</th>
  </tr>
  <% @toilets.each do |toilet| %>
      <tr>
        <td class="name"><%= toilet.name %></td>
        <td class="address"><%= toilet.address %></td>
        <td class="venuetype"><%= toilet.venuetype %></td>
        <td class="toiletlocation"><%= toilet.toiletlocation %></td>
        <td class="whocanuse"><%= toilet.whocanuse %></td>
        <td class="howtoaccess"><%= toilet.howtoaccess %></td>
        <td class="changingbench"><%= toilet.changingbench  %></td>
        <td class="hoist"><%= toilet.hoist  %></td>
        <td class="description"><%= toilet.description %></td>
        <td class="longitude"><%= toilet.long %></td>
        <td class="latitude"><%= toilet.lat %></td>
      </tr>
  <% end %>
</table>
